{
  "name": "detektiv-kollektiv",
  "version": "0.0.0",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=dev && compodoc -p tsconfig.json -d dist/documentation",
    "build:qa": "ng build --configuration=qa && compodoc -p tsconfig.json -d dist/documentation",
    "build:prod": "ng build --configuration=production && compodoc -p tsconfig.json -d dist/documentation",
    "test": "ng test",
    "test:headless": "ng test --watch=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "e2e:ci": "start-server-and-test start http://localhost:4200 cy:run",
    "compodoc": "npx compodoc -p tsconfig.json -d dist/documentation"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/core": "^9.1.6",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.1.6",
    "@angular/material": "^9.2.3",
    "@angular/material-moment-adapter": "^10.0.2",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@angular/router": "^9.1.6",
    "@briebug/jest-schematic": "^3.1.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "aws-amplify": "^3.0.10 ",
    "hammerjs": "^2.0.8",
    "lato-font": "^3.0.0",
    "moment": "^2.27.0",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.5.4",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.11.2",
    "typeface-oswald": "^1.1.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.1",
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "^9.1.5",
    "@angular/compiler-cli": "^9.1.6",
    "@angular/language-service": "^9.1.6",
    "@briebug/cypress-schematic": "^4.4.0",
    "@compodoc/compodoc": "^1.1.11",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "26.0.19",
    "@types/node": "^12.12.38",
    "codelyzer": "^5.1.2",
    "cypress": "6.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "26.6.3",
    "ng-packagr": "^9.0.0",
    "start-server-and-test": "^1.11.6",
    "ts-loader": "8.0.12",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
